<div class="chat">
  <div class="chat-window">
    <%
      room.messages.each do |message|
        %>
          <p><span><%= message.session.nick %>:</span> <%= message.text %><p>
        <%
      end
    %>
  </div>
  <aside class="users-list">
    <%
      room.sessions.each do |session|
        %>
          <a><%= session.nick %></a>
        <%
      end
    %>
  </aside>
  <div class="chat-bar">
  </div>
</div>
